/**
 * @param {number} n
 * @return {string[]}
 */
var fizzBuzz = function(n) {
    //n=3 - 1<n if i=1 1%3 && 1&&5 = "FinzzBuZZ" 1%3 = "fizz" 1%5= buzz  
    //for loop if i=1 <n O(n) arr[i]="fizzbuzz"
    let answer = [];
    for(let i=1;i<=n;i++){
       if(i%3 === 0 && i%5 === 0){
           answer.push("FizzBuzz");
       }else if(i%3 === 0){
         answer.push("Fizz");
       }else if(i%5 === 0){
        answer.push("Buzz");
       }else{
        
        answer.push(`${i}`);
        //covert i type
        
       }
    }
    return answer;
};
